{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/front/(.*)",
      "destination": "https://mock.apifox.cn/m1/2401164-0-default/$1"
    },
    {
      "source": "/login_process",
      "destination": "http://182.254.194.46:8765/login_process"
    },
    {
      "source": "/flask/(.*)",
      "destination": "https://your-backend-url.com/$1"
    },
    {
      "source": "/predict/(.*)",
      "destination": "https://your-predict-url.com/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}